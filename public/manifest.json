{
  "manifest_version": 2,
  "name": "Cookie Helper",
  "description": "抓取其他域名下的cookie注入到指定域名",
  "version": "1.0.0",
  "permissions": ["cookies", "storage", "<all_urls>"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Cookie Helper",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
